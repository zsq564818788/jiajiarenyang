.animal_wrapper {
    position: fixed;
    right: 240rpx;
    top: 400rpx;
}

.cicken1 {
    width: 60rpx;
    height: 60rpx;
    animation: run 20s linear infinite;
}

.top {
    top: 510rpx;
}

@-webkit-keyframes run {
    49% {
        transform: translateX(60px);
    }

    50% {
        transform: translateX(60px) rotateY(-180deg);
    }

    99% {
        transform: translateX(0px) rotateY(-180deg);
    }

    100% {
        transform: rotateY(0deg);
    }
}

@keyframes run {
    49% {
        transform: translateX(60px);
    }

    50% {
        transform: translateX(60px) rotateY(-180deg);
    }

    99% {
        transform: translateX(0px) rotateY(-180deg);
    }

    100% {
        transform: rotateY(0deg);
    }
}