.animal_wrapper {
    position: fixed;
    right: 350rpx;
    top: 280rpx;
}

.cicken3 {
    width: 50rpx;
    height: 46.5rpx;
    animation: run 12s linear infinite;
}

.top {
    top: 370rpx;
}

@-webkit-keyframes run {
    49% {
        transform: translateX(30px);
    }

    50% {
        transform: translateX(30px) rotateY(-180deg);
    }

    99% {
        transform: translateX(0px) rotateY(-180deg);
    }

    100% {
        transform: rotateY(0deg);
    }
}

@keyframes run {
    49% {
        transform: translateX(30px);
    }

    50% {
        transform: translateX(30px) rotateY(-180deg);
    }

    99% {
        transform: translateX(0px) rotateY(-180deg);
    }

    100% {
        transform: rotateY(0deg);
    }
}