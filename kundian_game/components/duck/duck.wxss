.animal_wrapper {
    position: fixed;
    left: 295rpx;
    top: 310rpx;
}

.top {
    top: 420rpx;
}

.duck1 {
    width: 50rpx;
    height: 56rpx;
    animation: run 15s linear infinite;
}

@-webkit-keyframes run {
    49% {
        transform: translateX(70px);
    }

    50% {
        transform: translateX(70px) rotateY(-180deg);
    }

    99% {
        transform: translateX(0px) rotateY(-180deg);
    }

    100% {
        transform: rotateY(0deg);
    }
}

@keyframes run {
    49% {
        transform: translateX(70px);
    }

    50% {
        transform: translateX(70px) rotateY(-180deg);
    }

    99% {
        transform: translateX(0px) rotateY(-180deg);
    }

    100% {
        transform: rotateY(0deg);
    }
}