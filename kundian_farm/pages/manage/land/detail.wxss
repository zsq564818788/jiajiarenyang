page {
    background: #f4f4f4;
}

.order-status {
    width: 90%;
    padding: 20rpx;
    height: 164rpx;
    margin: auto;
    border-radius: 10rpx;
    text-align: center;
    margin-top: 20rpx;
    background: white;
}

.cu-steps .cu-item .num-1 {
    height: 40rpx;
    border-radius: 50%;
    line-height: 40rpx;
    margin: 20rpx auto;
    font-size: 24rpx;
    border: 1px solid currentColor;
    position: relative;
    overflow: hidden;
    width: 40rpx;
    display: block;
}

.land-info {
    width: 90%;
    padding: 20rpx;
    background: white;
    margin: auto;
    border-radius: 10rpx;
    margin-top: 20rpx;
}

.title {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    border-bottom: 1rpx solid #f4f4f4;
    margin-bottom: 20rpx;
    font-weight: bold;
}

.cycle-title {
    width: 100%;
    height: 80rpx;
    justify-content: space-between;
    align-items: center;
}

.order-ul {
    width: 100%;
    height: 80rpx;
    border-bottom: 1rpx solid #f4f4f4;
    justify-content: space-between;
    align-items: center;
}

.seed-info {
    height: 110rpx;
    line-height: 110rpx;
    width: 100%;
}

.seed-cover {
    width: 100rpx;
    height: 100rpx;
}

.name {
    width: 35%;
    padding-left: 20rpx;
}

.area {
    width: 20%;
}

.status {
    width: 15%;
    text-align: right;
}

.detail {
    font-size: 24rpx;
    color: #868686;
    width: 15%;
    text-align: right;
}

.cu-dialog {
    width: 85%;
}

.cu-dialog .content {
    font-weight: bold;
    color: #000;
}

.s-li {
    width: 100%;
    height: 70rpx;
    line-height: 70rpx;
    justify-content: space-between;
    align-items: center;
}

.s-head {
    color: #868686;
}

.to-info {
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    color: #1E9FFF;
    font-size: 26rpx;
}