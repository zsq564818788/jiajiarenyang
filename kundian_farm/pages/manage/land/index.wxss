page {
    background: #f4f4f4;
}

.nav {
    position: fixed;
    top: 0;
    z-index: 99;
}

.land-info {
    width: 90%;
    margin: auto;
    background: white;
    border-radius: 10rpx;
    margin-top: 10rpx;
    padding: 20rpx;
    position: relative;
    overflow: hidden;
}

.user-info {
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 1rpx solid #f4f4f4;
}

.user-info image {
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
    margin-right: 20rpx;
    position: relative;
    top: 16rpx;
}

.land {
    width: 100%;
    height: 200rpx;
    padding: 20rpx 0 20rpx 0;
}

.land-img {
    width: 200rpx;
    height: 200rpx;
}

.state_container {
    padding: 30rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    font-size: 28rpx;
    text-align: center;
    justify-content: space-between;
}

.item_container {
    width: 33.3%;
    letter-spacing: 3rpx;
}

.expire {
    background-color: #fc8535;
    font-size: 28rpx;
    width: 150rpx;
    text-align: center;
    color: #fff;
    position: absolute;
    transform: rotate(45deg);
    right: -35rpx;
}