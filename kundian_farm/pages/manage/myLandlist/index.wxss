page {
    background-color: #efefef;
}

.land_item {
    width: 94%;
    margin: 20rpx auto;
    border-radius: 20rpx;
    background-color: #fff;
    overflow: hidden;
    position: relative;
}

.userInfo {
    width: 100%;
    padding: 20rpx 30rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
}

.landInfo {
    padding: 30rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    border-top: 1rpx solid #eaeaea;
    border-bottom: 1rpx solid #eaeaea;
    font-size: 32rpx;
}

.itemName {
    width: 400rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.landAvator {
    width: 200rpx;
    height: 200rpx;
}

.area {
    position: relative;
}

.state_container {
    padding: 30rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    font-size: 28rpx;
    text-align: center;
    justify-content: space-between;
}

.item_container {
    width: 33.3%;
    letter-spacing: 3rpx;
}

.block20 {
    height: 20rpx;
}