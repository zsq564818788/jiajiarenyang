page {
    background: #f4f4f4;
}

.order-status {
    width: 90%;
    padding: 20rpx;
    height: 164rpx;
    margin: auto;
    border-radius: 10rpx;
    text-align: center;
    margin-top: 20rpx;
    background: white;
}

.land-info {
    width: 90%;
    padding: 20rpx;
    background: white;
    margin: auto;
    border-radius: 10rpx;
    margin-top: 20rpx;
}

.title {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    border-bottom: 1rpx solid #f4f4f4;
    margin-bottom: 20rpx;
    font-weight: bold;
}

.land-info image {
    width: 280rpx;
    height: 180rpx;
}

.land-right {
    margin-left: 20rpx;
}

.icon-lanmanbiaoti {
    position: relative;
    top: 10rpx;
    font-size: 40rpx;
}

.order-ul {
    width: 100%;
    height: 80rpx;
    border-bottom: 1rpx solid #f4f4f4;
    justify-content: space-between;
    align-items: center;
}

.cycle-title {
    width: 100%;
    height: 80rpx;
    justify-content: space-between;
    align-items: center;
}

.cu-steps .cu-item .num-1 {
    height: 40rpx;
    border-radius: 50%;
    line-height: 40rpx;
    margin: 20rpx auto;
    font-size: 24rpx;
    border: 1px solid currentColor;
    position: relative;
    overflow: hidden;
    width: 40rpx;
    display: block;
}

.num-blue {
    background: #39b54a;
    color: white;
}

.land-cond .cover {
    width: 100%;
    height: 375rpx;
}

.l-mask {
    background-color: rgba(0,0,0,0.5);
    position: absolute;
    margin-top: -56rpx;
    height: 50rpx;
    width: 87.3%;
    font-size: 22rpx;
    padding-left: 20rpx;
    color: white;
    line-height: 50rpx;
}

.land-cond .monitor {
    position: absolute;
    left: 84%;
    margin-top: 10rpx;
    z-index: 99;
    width: 80rpx;
    height: 80rpx;
}

.land-cond video {
    width: 100%;
    height: 375rpx;
}

.live-play {
    width: 100%;
    height: 375rpx;
}