page {
    background: #f4f4f4;
}

.cond {
    background: white;
    width: 90%;
    margin: auto;
    padding: 20rpx;
    border-radius: 10rpx;
    margin-top: 20rpx;
}

.live-cond {
    height: 375rpx;
}

.live-cond .cover,video,live-player {
    width: 100%;
    height: 375rpx;
}

.monitor {
    position: absolute;
    width: 90rpx;
    height: 90rpx;
    margin-top: 10rpx;
    left: 84%;
    z-index: 99;
}

.head {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    font-weight: bold;
    font-size: 32rpx;
}

.land-li {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    justify-content: space-between;
    align-items: center;
}

.land-li .title {
    color: #868686;
}

.seed-li {
    width: 100%;
    height: 120rpx;
    line-height: 120rpx;
    border-bottom: 1rpx solid #f4f4f4;
}

.seed-li image {
    width: 90rpx;
    height: 90rpx;
    border-radius: 10rpx;
}

.name {
    width: 27%;
    margin-left: 20rpx;
}

.count {
    width: 20%;
}

.status,.detail {
    width: 20%;
}

.detail {
    text-align: right;
    color: gray;
    font-size: 24rpx;
}

.cu-dialog {
    width: 85%;
}

.cu-dialog .content {
    font-weight: bold;
    color: #000;
}

.s-li {
    width: 100%;
    height: 70rpx;
    line-height: 70rpx;
    justify-content: space-between;
    align-items: center;
}

.s-head {
    color: #868686;
}

.to-info {
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    color: #1E9FFF;
    font-size: 26rpx;
}

.op-web {
    position: fixed;
    bottom: 0;
    width: 100%;
    border-top: 1rpx solid #f4f4f4;
}

.cu-bar.tabbar .action .weed {
    width: 50rpx;
    height: 50rpx;
    margin-top: 10rpx;
}

.cu-bar.tabbar .action .Insecticide {
    height: 50rpx;
    width: 77.5rpx;
    margin-top: 10rpx;
}

.cu-bar.tabbar .action .fertilizer {
    height: 50rpx;
    width: 40rpx;
    margin-top: 10rpx;
}

.cu-bar.tabbar .action .watering {
    height: 50rpx;
    width: 76rpx;
    margin-top: 10rpx;
}

button {
    background: none;
    margin: 0;
    padding: 0;
    font-size: 28rpx;
}

button::after {
    border: none;
}

.situation {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-around;
    margin-top: 30rpx;
    font-size: 28rpx;
    text-align: center;
    color: #464646;
}

.cf888789 {
    color: #868686;
}

.humidity {
    height: 80rpx;
    width: 56rpx;
}

.temperature {
    height: 80rpx;
    width: 76rpx;
}

.co2 {
    height: 80rpx;
    width: 48rpx;
}

.Illumination {
    width: 80rpx;
    height: 80rpx;
}

.cycle-title {
    width: 100%;
    height: 80rpx;
    justify-content: space-between;
    align-items: center;
}

.order-ul {
    width: 100%;
    height: 80rpx;
    border-bottom: 1rpx solid #f4f4f4;
    justify-content: space-between;
    align-items: center;
}