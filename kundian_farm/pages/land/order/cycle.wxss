scroll-view {
    width: 95%;
    height: 160rpx;
    padding: 20rpx;
    white-space: nowrap;
}

.cycle-li {
    width: 100rpx;
    height: 100rpx;
    text-align: center;
    font-size: 24rpx;
    margin-left: 20rpx;
    display: inline-block;
    padding: 20rpx;
    margin-top: 10rpx;
    margin-right: 10rpx;
    border-radius: 10rpx;
}

.cycle-li view {
    margin-top: 10rpx;
}

.status {
    font-size: 20rpx;
}

.cycle-li .cu-tag {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    margin-left: -70rpx;
    margin-top: -21rpx;
    font-size: 18rpx;
}

.cycle-li .active-tag {
    color: white;
    background: red!important;
    border-radius: 10rpx 0 0 0;
}

.active-border {
    border: 1rpx solid red;
}

.cond {
    width: 90%;
    margin: auto;
}

.info-ul {
    width: 100%;
    height: 90rpx;
    border-bottom: 1rpx solid #f4f4f4;
    line-height: 90rpx;
}

.info-ul .title {
    width: 30%;
    color: #868686;
}

.footer {
    position: fixed;
    bottom: 0rpx;
    text-align: right;
    border-top: 1rpx solid #f4f4f4;
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
}

.footer .cu-btn {
    margin-right: 20rpx;
}