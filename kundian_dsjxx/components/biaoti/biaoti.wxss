/* components/biaoti/biaoti.wxss */
.biaoti{
    position: relative;
    border-bottom: 2rpx solid #0c89d5;
    -webkit-box-shadow: inset 0 0 10rpx rgba(7, 166, 255, 0.51);
    box-shadow: inset 0 0 10rpx rgba(7, 166, 255, 0.51);
    font-size: 32rpx;
    color: white;
    height: 43rpx;
}
.biaoti image{
    content: '';
    width: 14rpx;
    height: 12rpx;
    position: absolute;
    left: 11rpx;
    top: 50%;
    margin-top: -6rpx;
}
.biaoti view{
    padding-left: 32rpx;
    color: white;
    font-size: 34rpx;
    font-weight: 400;
    height: 43rpx;
    line-height: 46rpx;
    position: relative;
}