@import "../../wuBaseWxss/1.wxss";

.search-wrapper {
    width: 100%;
    background:#39b54a;
    display: flex;
    align-items: center;
    color: #fff;
    padding: 5px 0;
    z-index: -1;
    padding-bottom: 15px;
}

.search-input {
    width: 80%;
    line-height: 30px;
    font-size: 30rpx;
    margin-left: 50rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    /* z-index: -1; */
    padding: 2%;
    background-color: #61c46e;
    border-radius: 8rpx;
}

.search-wrapper .iconfont {
    line-height: 30px;
    display: inline-block;
    vertical-align: middle;
    box-sizing: border-box;
    position: relative;
    left: -40rpx;
}
.phcolor{
    color: #fff;
}