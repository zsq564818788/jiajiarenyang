.s-count {
    width: 80rpx;
    height: 40rpx;
    font-size: 24rpx;
    background-color: rgba(0,0,0,0.5);
    position: absolute;
    left: 87%;
    top: 24%;
    border-radius: 40rpx;
    line-height: 44rpx;
    text-align: center;
    color: white;
}

.con {
    width: 100%;
    position: relative;
    top: -20rpx;
    background: white;
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
    padding-bottom: 20rpx;
}

.s-li-one {
    width: 90%;
    margin-left: 5%;
    height: 120rpx;
    padding: 20rpx 0;
}

.name {
    font-size: 36rpx;
    font-weight: bold;
    width: 70%;
    height: 100%;
    border-right: 1rpx solid #f4f4f4;
    line-height: 120rpx;
}

.collect {
    width: 30%;
    text-align: center;
}

.s-li-desc {
    width: 90%;
    margin-left: 5%;
    height: 80rpx;
    padding: 20rpx 0;
    text-align: left;
}

.title {
    width: 100rpx;
}

.s-li-remark {
    width: 90%;
    margin-left: 5%;
    padding: 20rpx 0;
}

.s-li-phone {
    width: 90%;
    margin-left: 5%;
    height: 80rpx;
}

.phone {
    width: 70%;
    border-right: 1rpx solid #f4f4f4;
}

.li-icon {
    width: 30%;
    text-align: center;
}

.cu-tag {
    margin-top: 10rpx;
}

.icon-lanmanbiaoti {
    font-size: 60rpx;
    position: relative;
    top: 10rpx;
}

.live {
    width: 100%;
    height: auto;
}

.video-cover {
    width: 94%;
    height: 330rpx;
    margin-left: 3%;
}

.cu-list.grid>.cu-item {
    padding: 0;
}

.cu-list.grid>.cu-item .icon-play {
    position: absolute;
    margin-top: 42%;
    z-index: 9999;
    font-size: 90rpx;
    color: white;
}

.v-title {
    position: absolute;
    margin-top: 77%;
    z-index: 9999;
    font-size: 26rpx;
    color: white;
    width: 94%;
    height: 50rpx;
    background-color: rgba(0,0,0,0.5);
    margin-left: 3%;
    line-height: 50rpx;
    overflow: hidden;
}

.cu-item {
    width: 90%;
}

.erweima {
    position: absolute;
    top: 20rpx;
    height: 60rpx;
    width: 60rpx;
    color: white;
    background-color: rgba(0,0,0,0.5);
    left: 90%;
    text-align: center;
    border-radius: 6rpx;
}

.head {
    width: 100%;
    height: 150rpx;
    text-align: center;
}

.head .head-img {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    position: relative;
    top: 15rpx;
}

.h-v {
    width: 150rpx;
    height: 160rpx;
    background: white;
    margin: auto;
    position: relative;
    top: -60rpx;
    border-radius: 50%;
}

.top-icon {
    width: 100%;
    height: 60rpx;
    position: absolute;
    top: 20rpx;
    display: flex;
    text-align: right;
}

.icon-right {
    position: absolute;
    left: 90%;
}

.page-view {
    position: relative;
    top: -70rpx;
    color: gray;
}

.address {
    width: 80%;
    height: auto;
}

.post-mask {
    width: 100%;
    height: 100%;
    background: black;
    opacity: 0.5;
    position: fixed;
    top: 0;
    z-index: 99;
}

.post {
    width: 90%;
    height: 1050rpx;
    margin-left: 5%;
    background: white;
    position: fixed;
    top: 2%;
    border-radius: 10rpx;
    z-index: 999;
    text-align: center;
}

.post image {
    width: 100%;
    height: 900rpx;
    border-radius: 10rpx;
}

.post .cu-btn {
    margin-top: 40rpx;
    width: 70%;
    margin-left: 20rpx;
}

.post .icon-delete1 {
    color: white;
    position: relative;
    top: 70rpx;
    font-size: 42rpx;
    display: block;
}

.create-btn {
    width: 70%;
    margin-left: 15%;
    height: 70rpx;
    font-size: 36rpx;
}

.block {
    width: 100%;
    height: 20rpx;
    background: #f4f4f4;
}