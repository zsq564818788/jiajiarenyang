.s-count {
    width: 80rpx;
    height: 40rpx;
    font-size: 24rpx;
    background-color: rgba(0,0,0,0.5);
    position: absolute;
    left: 87%;
    top: 35%;
    border-radius: 40rpx;
    line-height: 44rpx;
    text-align: center;
    color: white;
}

.screen-swiper {
    height: 480rpx;
}

.editSlide,.storePost {
    margin-left: 78%;
    top: 20rpx;
    font-size: 46rpx;
    color: white;
    background-color: rgba(0,0,0,0.5);
    width: 60rpx;
    text-align: center;
    border-radius: 5rpx;
    height: 60rpx;
}

.storePost {
    margin-left: 20rpx;
}

.head {
    width: 100%;
    height: 150rpx;
    text-align: center;
}

.head .head-img {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    position: relative;
    top: 15rpx;
}

.h-v {
    width: 150rpx;
    height: 160rpx;
    background: white;
    margin: auto;
    position: relative;
    top: -60rpx;
    border-radius: 50%;
}

.top-icon {
    width: 100%;
    height: 60rpx;
    position: absolute;
    top: 20rpx;
    display: flex;
    text-align: right;
}

.icon-right {
    position: absolute;
    left: 90%;
}

.con {
    border-top: 1rpx solid #f4f4f4;
    padding-bottom: 50rpx;
    overflow-y: auto;
    height: auto;
}

.icon-lanmanbiaoti {
    font-size: 60rpx;
    position: relative;
    top: 10rpx;
}

.block {
    width: 100%;
    height: 20rpx;
    background: #f4f4f4;
}

.video-cover {
    width: 94%;
    height: 330rpx;
    margin-left: 3%;
}

.cu-list.grid>.cu-item {
    padding: 0;
}

.cu-list.grid>.cu-item .icon-play {
    position: absolute;
    margin-top: 42%;
    z-index: 9999;
    font-size: 90rpx;
    color: white;
}

.v-title {
    position: absolute;
    margin-top: 77%;
    z-index: 9999;
    font-size: 26rpx;
    color: white;
    width: 94%;
    height: 50rpx;
    background-color: rgba(0,0,0,0.5);
    margin-left: 3%;
    line-height: 50rpx;
    overflow: hidden;
}

.cu-list.grid>.cu-item .v-edit {
    position: absolute;
    margin-top: 9rpx;
    z-index: 9999;
    margin-left: 58%;
    width: 30px;
    height: 60rpx;
    background-color: rgba(0,0,0,0.5);
    border-radius: 50%;
}

.cu-list.grid>.cu-item .v-del {
    position: absolute;
    margin-top: 9rpx;
    z-index: 9999;
    margin-left: 77%;
    width: 30px;
    height: 60rpx;
    background-color: rgba(0,0,0,0.5);
    border-radius: 50%;
}

.cu-list.grid>.cu-item .icon-modify,.cu-list.grid>.cu-item .icon-el-icon-dev-delete {
    color: white;
    margin-top: 15rpx;
    font-size: 34rpx;
}

.page-view {
    position: relative;
    top: -60rpx;
    color: gray;
}

.post-mask {
    width: 100%;
    height: 100%;
    background: black;
    opacity: 0.5;
    position: fixed;
    top: 0;
    z-index: 99;
}

.post {
    width: 90%;
    height: 1050rpx;
    margin-left: 5%;
    background: white;
    position: fixed;
    top: 2%;
    border-radius: 10rpx;
    z-index: 999;
    text-align: center;
}

.post image {
    width: 100%;
    height: 900rpx;
    border-radius: 10rpx;
}

.post .cu-btn {
    margin-top: 40rpx;
    width: 35%;
    margin-left: 20rpx;
}

.post .icon-delete1 {
    color: white;
    position: relative;
    top: 70rpx;
    font-size: 42rpx;
    display: block;
}

.create-btn {
    width: 70%;
    margin-left: 15%;
    height: 70rpx;
    font-size: 36rpx;
}