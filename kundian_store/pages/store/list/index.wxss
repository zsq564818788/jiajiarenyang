.cu-card.article>.cu-item {
    padding: 0;
}

.con {
    padding-bottom: 40rpx;
}

.s-li {
    width: 90%;
    margin-left: 5%;
    height: 220rpx;
    border-radius: 10rpx;
    display: flex;
    margin-top: 20rpx;
}

.logo {
    width: 220rpx;
    height: 220rpx;
    border-top-left-radius: 10rpx;
    border-bottom-left-radius: 10rpx;
}

.s-info {
    width: 60%;
    margin-left: 20rpx;
    padding-top: 20rpx;
}

.s-name {
    width: 100%;
    height: 60rpx;
    overflow: hidden;
    white-space: nowrap;
}

.name {
    font-size: 32rpx;
    width: 100%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.desc {
    font-size: 24rpx;
    height: 40rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.remark {
    font-size: 22rpx;
    color: gray;
}

.float-btn {
    position: fixed;
    left: 81%;
    top: 85%;
    border-radius: 50%;
    width: 90rpx;
    height: 90rpx;
    text-align: center;
    color: white;
    line-height: 30rpx;
    font-size: 22rpx;
}

.cu-tag {
    letter-spacing: 8rpx;
}