.img-li {
    width: 100%;
    height: 159rpx;
}

.grid.grid-square>view {
    margin-right: 12rpx;
}

.cu-form-group {
    width: 84%;
    margin-left: 4%;
    border-radius: 10rpx;
}

.ul-remark {
    width: 92%;
    margin-left: 4%;
    border-radius: 10rpx;
